{
  "name": "harrypotter-names",
  "version": "1.0.0",
  "description": "This project gives you names from the Harry Potter movie",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha",
    "watch:test": "mocha --watch",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kreuzerk/HarryPotter-names.git"
  },
  "keywords": [
    "Harry",
    "Potter",
    "Names",
    "Random"
  ],
  "author": "Kevin Kreuzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kreuzerk/HarryPotter-names/issues"
  },
  "homepage": "https://github.com/kreuzerk/HarryPotter-names#readme",
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.14.0",
    "eslint-config-kentcdodds": "^9.0.0",
    "ghooks": "^2.0.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
