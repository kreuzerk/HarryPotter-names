{
  "name": "harrypotter-names",
  "version": "1.0.0",
  "description": "This project gives you names from the Harry Potter movie",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "watch:test": "mocha --watch",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel lint test build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kreuzerk/HarryPotter-names.git"
  },
  "keywords": [
    "Harry",
    "Potter",
    "Names",
    "Random"
  ],
  "files": [
    "dist"
  ],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kreuzerk/HarryPotter-names/issues"
  },
  "homepage": "https://github.com/kreuzerk/HarryPotter-names#readme",
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-istanbul": "1.0.3",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "6.11.6",
    "chai": "^3.5.0",
    "cross-env": "2.0.0",
    "eslint": "^3.14.0",
    "eslint-config-kentcdodds": "^9.0.0",
    "ghooks": "^2.0.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "rimraf": "^2.5.4"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ],
    "sourceMap": false,
    "instrument": false,
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
